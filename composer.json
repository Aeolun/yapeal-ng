{
    "name": "yapeal/yapeal-ng",
    "description": "Yet Another Php Eve Api Library Next Generation also know as Yapeal-ng which can be used to access the Eve Online API data and place it into a database.",
    "keywords": [
        "curl",
        "database",
        "Eve",
        "Eve-Online",
        "mysql",
        "PDO",
        "REST",
        "RESTful"
    ],
    "license": "LGPL-3.0+",
    "autoload": {
        "psr-4": {
            "Yapeal\\": [
                "lib/",
                "bin/"
            ]
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Spec\\": [
                "specs/"
            ]
        }
    },
    "authors": [
        {
            "name": "Michael Cummings",
            "email": "mgcummings@yahoo.com",
            "role": "Developer"
        }
    ],
    "bin": [
        "bin/yc"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "homepage": "https://github.com/Yapeal/yapeal",
    "support": {
        "forum": "https://forums.eveonline.com/default.aspx?g=posts&t=360758",
        "issues": "https://github.com/Yapeal/yapeal-ng/issues?state=open",
        "source": "https://github.com/Yapeal/yapeal-ng",
        "wiki": "https://github.com/Yapeal/yapeal-ng/wiki"
    },
    "require": {
        "dragonrun1/file_path_normalizer": "1.1.*",
        "dragonrun1/event-mediator": "<1.1.0",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-hash": "*",
        "ext-openssl": "*",
        "ext-PDO": ">=1.0.0",
        "ext-pdo_mysql": "*",
        "ext-Reflection": "*",
        "ext-SimpleXML": "*",
        "ext-SPL": "*",
        "ext-tidy": "*",
        "ext-xmlreader": "*",
        "ext-xsl": "*",
        "lib-curl": ">=7.15.0",
        "lib-openssl": ">=0.9.8",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "~1.11",
        "php": ">=7.0.0,<7.2",
        "symfony/console": "~2.5",
        "symfony/yaml": "~2.5",
        "twig/twig": "<2.0"
    },
    "require-dev": {
        "dragonrun1/semantic-version": "master@dev",
        "dragonrun1/phpspec": "2.1.*@dev",
        "erusev/parsedown": "<1.6",
        "henrikbjorn/phpspec-code-coverage": "1.0.1",
        "mikey179/vfsStream": "~1.4",
        "phploc/phploc": "<2.3",
        "phpspec/nyan-formatters": "1.0.1",
        "satooshi/php-coveralls": "dev-master"
    }
}
